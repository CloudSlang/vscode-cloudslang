{
    "name": "vscode-cloudslang",
    "version": "1.0.0",
    "displayName": "CloudSlang Language",
    "description": "CloudSlang Language Support",
    "keywords": [
        "automation",
        "devops",
        "orchestration",
        "language",
        "grammar"
    ],
    "license": "Apache v2.0",
    "icon": "images/cloudslang-icon.png",
    "homepage": "http://atom.github.io/language-cloudslang",
    "publisher": "MicroFocus",
    "engines": {
        "vscode": "^1.18.0"
    },
    "categories": [
        "Languages",
        "Snippets"
    ],
    "contributes": {
        "languages": [{
            "id": "cloudslang",
            "aliases": ["CloudSlang", "cloudslang"],
            "extensions": ["sl","sl.yml","sl.yaml"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "cloudslang",
            "scopeName": "source.sl",
            "path": "./syntaxes/cloudslang.tmLanguage.json"
        }],
        "snippets": [{
            "language": "cloudslang",
            "path": "./snippets/snippets.json"
        }]
    }
}